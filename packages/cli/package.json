{
  "name": "@gbvflabs/cli",
  "version": "0.0.0",
  "private": false,
  "bin": {
    "gbvflabs": "src/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf node_modules && rimraf dist && mkdir -p dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "24",
    "pnpm": "10"
  },
  "dependencies": {
    "@gbvflabs/dep": "workspace:^",
    "@gbvflabs/util": "workspace:^",
    "@gbvflabs/core": "workspace:^",
    "@gbvflabs/sync": "workspace:^"
  },
  "devDependencies": {}
}
