{
  "name": "@gbvflabs/cli",
  "version": "0.1.2",
  "private": false,
  "bin": {
    "gbvflabs": "dist/index.js"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "prerelease": "pnpm build && pnpm version prerelease --preid=beta && pnpm publish --tag beta",
    "release": "pnpm build && pnpm version patch && pnpm publish",
    "clean": "rimraf node_modules && rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "24",
    "pnpm": "10"
  },
  "dependencies": {
    "@gbvflabs/dep": "workspace:^"
  },
  "devDependencies": {}
}
