{
  "name": "@gbvflabs/cli",
  "version": "0.1.1",
  "private": false,
  "bin": {
    "gbvflabs": "src/index.ts"
  },
  "main": "dist/src.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["src", "README.md", "LICENSE"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "pkg:publish": "npm publish",
    "pkg:version:patch": "npm version patch",
    "build": "tsup src/index.ts --format esm --target node24 --platform node --sourcemap --clean",
    "dev": "tsup src/index.ts --format esm --target node24 --platform node --watch --sourcemap",
    "cli": "node ./src/index.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "24",
    "pnpm": "10"
  },
  "dependencies": {
    "@arc/sync": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}
